jQuery(document).ready(function($) {
	if($('label.USER_BIRTHDAY').length)
	{
		$('label.USER_BIRTHDAY select').each(function() {
			$(this).selectmenu().selectmenu('menuWidget').addClass('jqUISelectoverflow');
		});
	}
	if($('label.jqUiRadio').length)
	{
		$('label.jqUiRadio .radio').each(function() {
			$(this).buttonset();
		});
	}
	if($('label.jqUiSlider').length)
	{
		$('label.jqUiSlider').each(function() {
			var t 			= $(this),
				max 		= t.data('max'),
				sl 			= $('.slider', t),
				input 		= $('input', t),
				val_txt 	= $('.val', t),
				val 		= (max/2);

			input.val(val);
			val_txt.html(val);

			sl.slider({
				orientation: 	'horisontal',
				range: 			'min',
				min: 			0,
				max: 			max,
				step: 			1,
				value: 			val,
				slide: 			function( event, ui ) {
					input.val(ui.value);
					val_txt.html(ui.value);
				}
			});
		});
	}

	function prOfferBeforeSize()
	{
		$('.r_test_result .item .offer').each(function() {
			var t = $(this),
				h = t.height(),
				b = $('.before', t);
			b.height(h);
		});
	}
	if($('.r_test_result .item .offer').length)
	{
		prOfferBeforeSize();
	}


	$(window).resize(function() {
		prOfferBeforeSize();
	});

});

function prTestNextAnswer(data)
{
	var q 		= $('.q_' + data.ID),
		h 		= q.outerHeight(true),
		n 		= q.next('.item'),
		i 		= n.data('i'),
		part 	= n.data('part'),
		data 	= $('.r_test_questions').data('data');

	var part_w 	= {1:453, 2:165, 3:282},
		human_w	= {1:20,2:545,3:705},
		left 	= 59;

	if(n.length)
	{
		var calc 	= part_w[part] / data[part] * (i - 1);
		
		$('.humans div').removeClass('active');
		$('.humans .part_' + part).addClass('active');

		$('.parts div').removeClass('active');
		$('.parts .part_' + part).addClass('active');

		if(calc > part_w[part]) calc = part_w[part];
		if(calc < left) calc = left;

		if(part == 1)
		{
			human_w[1] = human_w[1] + calc;
		}
		else if(part == 2)
		{
			left = left + part_w[1];
			human_w[1] = part_w[1];
			human_w[2] = human_w[2] + calc;
		}
		else if(part == 3)
		{
			left = left + part_w[1] + part_w[2];
			human_w[1] = part_w[1];
			human_w[2] = part_w[1] + part_w[2];
			human_w[3] = human_w[3] + calc;
		}

		$('.humans .part_1').css('left', human_w[1] + 'px');
		$('.humans .part_2').css('left', human_w[2] + 'px');
		$('.humans .part_3').css('left', human_w[3] + 'px');

		$('.r_test_progress .progress_h').css({
			'width' : calc + 'px',
			'left' : left + 'px',
		});

		q.fadeOut(300, function() {
			n.fadeIn();	
		});
	}
	else
	{
		location.reload();
	}
}
