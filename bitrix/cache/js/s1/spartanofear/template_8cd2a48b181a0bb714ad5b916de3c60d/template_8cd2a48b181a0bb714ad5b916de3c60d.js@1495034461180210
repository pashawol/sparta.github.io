jQuery(document).ready(function($) {

	/* FancyBox */
	fbImages 	= 'a[href$=".jpeg"]:not(.nofancybox),a[href$=".JPEG"]:not(.nofancybox),a[href$=".jpg"]:not(.nofancybox),a[href$=".JPG"]:not(.nofancybox),a[href$=".gif"]:not(.nofancybox),a[href$=".GIF"]:not(.nofancybox),a[href$=".png"]:not(.nofancybox),a[href$=".PNG"]:not(.nofancybox),a[href$=".bmp"]:not(.nofancybox),a[href$=".BMP"]:not(.nofancybox)';
	fbSettingG 	= {
		padding		: 0,
		topRatio	: 0,
		helpers:
		{
			overlay:
			{
				css:
				{
					'background' : 'rgba(0, 0, 0, 0.5)'
				},
				locked: false
			}
		}
	}
	$(fbImages).addClass('fancybox').attr('rel', 'gallery');
	if($('.fancybox').length > 0)
	{
		$('.fancybox').fancybox(fbSettingG);
	}

	/* Size */
	function prContainer()
	{
		var c_h = $('.container').outerHeight();
		var d_h = $(document).height();
		var f_h = $('.r_footer').outerHeight();

		if(c_h < d_h)
		{
			$('.container').css({'height' : d_h + 'px'});
		}
		if((d_h - f_h) > f_h)
		{
			$('.container').css({'margin-bottom' : '-' + f_h + 'px'});
		}
		else
		{
			$('.container').css({'margin-bottom' : '0px'});
		}
		$('.r_footer_spacer').css({'height' : f_h + 'px'});

	}
	prContainer();

	$(window).resize(function() {
		prContainer();
	});

	/* FP Slider*/
	function prFPSlider()
	{
		if($('.r_slider').length > 0)
		{
			var w_h 		= $(window).outerHeight(),
				r_top_h 	= $('.r_top').outerHeight(),
				set_h 		= w_h - r_top_h;

			if(set_h < 580) set_h = 580;
			$('.r_slider, .r_slider_spacer').css('height', set_h);
			$(window).resize(function() {
				w_h 		= $(window).outerHeight();
				set_h 		= w_h - r_top_h;
				if(set_h < 580) set_h = 580;
				if(set_h != 580)
				{
					$('.r_slider, .r_slider_spacer').css('height', set_h);
				}
			});
			$(window).load(function() {
				$(window).scroll(function() {
					var t_o = 1 - $(window).scrollTop() / 450;
					$('.r_slider').css({
						'opacity' : t_o
					});
				});
			});
		}
	}
	prFPSlider();

	/* Sidebars */
	if($('.sb-slidebar').length > 0)
	{
		$.slidebars();
	}

	/* Slider */
	$('.swiper-container').each(function() {
		var t 		= $(this),
			t_d 	= t.data('data'),
			t_nav 	= t.closest('.row').find('.nav');

		t_d.slideActiveClass = 'active';

		if(t_nav.length > 0)
		{
			t_d.prevButton = t_nav.find('.left')[0];
			t_d.nextButton = t_nav.find('.right')[0];
		}

		if(t_d.type === 'fpSlider')
		{
			/*
			var t_info 	= $('.info', t);
			t_d.onSlideChangeStart = function(swiper) {
				t_info.stop(true, true).fadeOut();
			};
			t_d.onSlideChangeEnd = function(swiper) {
				var g_data = $('.active', t).data('data');
				t_txt.html(g_data.txt);
				t_title.html(g_data.title);
				t_lnk.attr('href', g_data.lnk).data('id', g_data.id).attr('data-id', g_data.id);
				t_info.fadeIn();
			};
			*/
		}

		if(t_d.type === 'fpSliderQuotes')
		{
			var t_txt 	= $('.txt div', t);
			var t_red 	= $('.red', t);
			t_d.onSlideChangeStart = function(swiper) {
				t_txt.stop(true, true).fadeOut();
				t_red.stop(true, true).css('margin-top', '-400px');
			};
			t_d.onSlideChangeEnd = function(swiper) {
				var g_data = $('.active', t).data('data');
				t_txt.fadeIn().html(g_data.txt);
				t_red.stop(true, true).css('margin-top', '0px');
			};
		}

		var prSwiper = new Swiper (t, t_d);
	});

	/* Change Land */
	$('.r_top .lang select').change(function(){
		window.location.href = $(this).val();
	});

	/* Long Text */
	$('.jqLongText').hover(function(){
		var t_t = $(this).attr('title');
		var t_c = $(this).html();
		$(this).attr('title', t_c).html(t_t);
	}, function()
	{
		var t_t = $(this).attr('title');
		var t_c = $(this).html();
		$(this).attr('title', t_t).html(t_c);
	});

	/* Check All */
	$('.jqCheckAll input[type="checkbox"].first').click(function() {
		var t 	= $(this);
		var t_v = t.val();
		var t_p = t.closest('.jqCheckAll');
		if(t.prop("checked"))
		{
			$('input[type="checkbox"]', t_p).prop('checked', true);
		}
		else
		{
			$('input[type="checkbox"]', t_p).removeAttr('checked');
		}
	});

	/* Open Country */
	$(document).on('click', '.popup .locations .country_list .btn', function() {
		if($(this).hasClass('active')) return false;
		var t_p = $(this).closest('li');
		$('.popup .locations .country_list .btn').removeClass('active');
		$('.popup .locations .country_list ul').slideUp();
		$(this).addClass('active');
		$('ul', t_p).stop(true).slideToggle();
	});
	$(document).on('submit', '.popup .locations form', function() {
		var t 		= $(this);
		var v 		= $('input[type="text"]', t).val();
		var s 		= false;
		var id 		= false;
		var name 	= false;
		var dev 	= false;
		var red 	= false;

		v = v.charAt(0).toUpperCase() + v.substr(1);

		$('.popup .locations li a').each(function() {
			if($(this).data('name') == v)
			{
				id 		= $(this).data('id');
				name 	= $(this).data('name');
				def 	= $(this).data('def');
				red 	= $(this).attr('href');
				s = true;
				return;
			}
		});
		if(s === false)
		{
			window.open('/partner/?c=0', '_blank');
		}
		else
		{
			if(def == 'Y')
			{
				window.open('/partner/?c=' + name, '_blank');
			}
			else
			{
				window.open(red, '_blank');
			}
		}
		return false;
	});

	/* Ya Map */
	if($('#jqMapContacts').length > 0)
	{
		var cords = $('#jqMapContacts').data('cords').split(',');
		ymaps.ready(function () {
			var myMap = new ymaps.Map('jqMapContacts', {
			center: cords,
			zoom: 16
			}, {
			searchControlProvider: 'yandex#search'
			}),
			myPlacemark = new ymaps.Placemark(
				myMap.getCenter(),
				{
				hintContent: 'Sparta'
				}
			);

			myMap.geoObjects.add(myPlacemark);
		});
	}

	$(document).on('click', '.grid_accordion .title', function() {
		if($(this).closest('.item').hasClass('active'))
			return false;

		$(this).closest('.grid_accordion').find('.item').removeClass('active');
		$(this).closest('.grid_accordion').find('.txt').slideUp();
		$(this).closest('.item').addClass('active').find('.txt').slideDown();
	});

	if($('.r_calendar').length > 0)
	{
		$('.r_calendar .events a').each(function() {
			var t_p = $(this).closest('.col');
			var t_i = $(this).data('img');
			if(t_i.length > 0)
			{
				$(this).bind("mouseover", function(){
					t_p.css('background-image', 'url(' + t_i + ')');
				});
				$(this).bind("mouseout", function(){
					t_p.css('background-image', '');
				});

			}
		});
	}
	$('.jqNavToggle').on('click', function() {
		$($(this).data('toggle')).slideToggle();
	});
});
