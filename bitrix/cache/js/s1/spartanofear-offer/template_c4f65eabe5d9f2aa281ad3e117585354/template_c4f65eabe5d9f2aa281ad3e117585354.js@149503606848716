			if(t_i.length > 0)
			{
				$(this).bind("mouseover", function(){
					t_p.css('background-image', 'url(' + t_i + ')');
				});
				$(this).bind("mouseout", function(){
					t_p.css('background-image', '');
				});

			}
		});
	}
